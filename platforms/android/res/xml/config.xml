<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="HotCodePush">
    <param name="android-package" value="com.nordnetab.chcp.main.HotCodePushPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="HotCodePushLocalDevMode">
    <param name="android-package" value="com.nordnetab.chcp.localdev.HotCodePushLocalDevPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="FileChooser">
    <param name="android-package" value="com.megster.cordova.FileChooser"/>
  </feature>
  <allow-navigation href="cdvfile:*"/>
  <name>HelloCordova</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <allow-intent href="market:*"/>
  <preference name="loglevel" value="DEBUG"/>
  <preference name="loadUrlTimeoutValue" value="60000"/>
  <chcp>
    <local-development enabled="true"/>
    <config-file url="https://7ffb21a0.ngrok.io/chcp.json"/>
  </chcp>
</widget>